    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/erc20-tracker/bin/Debug/netcoreapp2.2/erc20-tracker.dll",
            "args": [],
            "cwd": "${workspaceFolder}/erc20-tracker",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ContractAddresses:0": "0xAbC0ecdaD3c1EFc21a6857F288def37CD5439d80",
                "TrackedAddresses:0": "0xde769dda17eD7d2178a5646D279382d0CaEC8079",
                "TrackedAddresses:1": "0xD5eaddD9462D2138389d3Ca281c3a10c952267E5",
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}